{
  "version": 2,
  "builds": [
    {
      
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
         "buildCommand": "npm install && npm run build",
        "installCommand": "npm install --legacy-peer-deps",
        "outputDirectory": "build",
        "devCommand": "npm start"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ],

  "git": {
    "productionBranch": "main"
  }
}
