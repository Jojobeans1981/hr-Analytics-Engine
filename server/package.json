{
  "name": "talent-risk-server",
  "version": "1.0.0",
  "description": "Backend server for talent risk assessment platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch server.tsx",
    "build": "tsc",
    "start": "node dist/server.js",
    "heroku-postbuild": "npm run build",
    "seed": "tsx seed-main.ts",
    "seed:clear": "tsx seed-main.ts --clear",
    "seed:50": "tsx seed-main.ts --count=50",
    "seed:100": "tsx seed-main.ts --count=100",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"No linter configured\"",
    "clean": "rm -rf dist node_modules package-lock.json",
    "railway": "./railway-start.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@tensorflow/tfjs": "^4.22.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "mongodb": "^6.21.0",
    "mongoose": "^8.19.4",
    "morgan": "^1.10.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/mongodb": "^4.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.25",
    "@types/nodemailer": "^7.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "talent-risk",
    "analytics",
    "nodejs",
    "express",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}
